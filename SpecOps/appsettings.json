{
  "AppSettings": {
    "AdminGroups": [ "mydomain\\abc", "mydomain\\def" ],
    "UserGroups": [ "mydomain\\uvw", "mydomain\\xyz" ],
    "OutputStyles": {
      "Data": "text-info",
      "Error": "text-danger",
      "Warning": "text-warning",
      "Info": "text-light",
      "Progress": "text-success",
      "Verbose": "text-teal",
      "Debug": "text-primary",
      "System": "text-info",
      "Unknown": "text-warning"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/SpecOps.txt",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "LocalDev"
    }
  }
}



